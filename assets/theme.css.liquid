.page-header { padding: 50px; }
.ttu { text-transform: uppercase; }
.ib { display: inline-block; }
.nm { margin: 0; }
.nmt { margin-top: 0; }
.nmb { margin-bottom: 0; }
body { 
  margin: 0;
  color: black;
}
html { padding-bottom: 0 !important; }
.tar { text-align: right; }
.fw { width: 100%; }
.flex-container, .flex-container-no{
  display: flex;
  align-items: center;
}
.tac { text-align: center; }
.subscript {font-size: 8pt;}
.half { width: 50%; }
.two-thirds { width: 66%; }
.fe { align-self: flex-end; }
.jes { justify-content: space-evenly; }
.jcc { justify-content: center; }
.accent { 
  font-family: chippewa-falls, sans-serif;
  font-weight: 500;
  font-style: normal
}
h1,h2,h3,h4,h5,a, span, input, th, select { 
  font-family: futura-pt-bold, sans-serif;
  font-weight: 700;
  font-style: normal;
}
p, textarea, td, ul, li, .error_message, .success_message { 
  font-family: futura-pt, sans-serif;
  font-weight: 400;
  font-style: normal;
} 

@media only screen and (min-width: 1100px) {
  h1 { font-size: 67pt; } 
  h2 { font-size: 34pt; } 
  h3{ font-size: 16pt; } 
  p, table { font-size: 14pt; } 
} 
@media only screen and (min-width: 800px) and (max-width: 1099px) {
  h1 { font-size: 57pt; } 
  h2 { font-size: 32pt; } 
  h3{ font-size: 14pt; } 
  p, table { font-size: 12pt; } 
}
@media only screen and (min-width: 589px) and (max-width: 799px) {
  h1 { font-size: 43pt; } 
  h2 { font-size: 30pt; } 
  p, table { font-size: 12pt; }
}
@media only screen and (min-width: 589px) {
  .mo { display: none; }
  .accent-padding { padding: 20px; }
}
@media only screen and (max-width: 588px) {
  h1 { font-size: 22pt; } 
  h2 { font-size: 16pt; } 
  .flex-container { display: block; }
  .flex-container .half { width: 100%; }
  .do { display: none; }
  .accent-padding { padding: 10px; }
  .half { width: 100%; }
}



